{
    "name": "yii-extension/user-translations",
    "type": "library",
    "description": "Translations for module user",
    "keywords": [
        "yii3",
        "translations",
        "module",
        "user"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/translator-formatter-intl": "^3.0@dev",
        "yiisoft/translator-message-gettext": "^3.0@dev"
    },
    "require-dev": {
        "httpsoft/http-message": "^1.0",
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.7",
        "vimeo/psalm": "^4.3",
        "yii-extension/user": "dev-main",
        "yii-extension/user-view-bootstrap5": "^1.0@dev",
        "yii-extension/user-view-bulma": "^1.0@dev",
        "yiisoft/router-fastroute": "^3.0@dev"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": [
                "config/common/*.php"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
